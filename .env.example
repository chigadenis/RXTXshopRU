# Payment API Configuration
REACT_APP_PAYMENT_API_KEY=your_payment_api_key_here
REACT_APP_PAYMENT_PROVIDER=yookassa # или другой провайдер (sberbank, tinkoff, etc.)

# Environment
REACT_APP_ENV=development

# API URLs
REACT_APP_API_BASE_URL=https://your-backend-api.com
REACT_APP_WEBHOOK_URL=https://your-backend-api.com/webhooks/payment

# Return URLs  
REACT_APP_PAYMENT_SUCCESS_URL=https://your-site.com/payment/success
REACT_APP_PAYMENT_FAILURE_URL=https://your-site.com/payment/failure